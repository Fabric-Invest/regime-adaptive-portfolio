# Fabric Platform
# Note: FABRIC_API_KEY is automatically injected by fabric-cli dev via docker-compose.yaml
# Only set this manually for production deployment or non-CLI usage
# FABRIC_API_KEY=your_fabric_api_key_here

# Development mode
FABRIC_DEV_MODE=true

# DEX Execution (0x)
ZERO_X_API_KEY=your_0x_api_key_here

# EVM Wallet
WALLET_PRIVATE_KEY=your_wallet_private_key_here

# Optional: Binance API for funding rates
# Public endpoints may work without authentication
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_SECRET_KEY=your_binance_secret_key_here

# Logging
LOG_LEVEL=INFO
